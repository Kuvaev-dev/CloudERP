@model DatabaseAccess.tblUserType

@{
    ViewBag.Title = "User Type Details";
}

<div class="form-horizontal">
    <div>
        <dl class="dl-horizontal">
            <dt>
                @Html.LabelFor(model => model.UserType, "User Type", htmlAttributes: new { @class = "control-label col-md-2" })
            </dt>

            <dd class="control-label col-md-2">
                @Html.DisplayFor(model => model.UserType)
            </dd>
        </dl>
    </div>
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            @Html.ActionLink("Edit", "Edit", new { id = Model.UserTypeID }, new { @class = "btn btn-primary" })
            @Html.ActionLink("Back to List", "Index", null, new { @class = "btn btn-default" })
        </div>
    </div>
</div>