@model Domain.Models.Supplier
@using Localization.CloudERP.Modules.Supplier
@using Localization.CloudERP.Modules.Miscellaneous
@{
    ViewBag.Title = Supplier.SupplierDetails;
}

<div>
    <dl class="dl-horizontal">
        <dt>
            @Supplier.SupplierName
        </dt>

        <dd>
            @Html.DisplayFor(model => model.SupplierName)
        </dd>

        <dt>
            @Supplier.ConatctNo
        </dt>

        <dd>
            @Html.DisplayFor(model => model.SupplierConatctNo)
        </dd>

        <dt>
            @Supplier.Address
        </dt>

        <dd>
            @Html.DisplayFor(model => model.SupplierAddress)
        </dd>

        <dt>
            @Localization.CloudERP.Modules.User.User.Email
        </dt>

        <dd>
            @Html.DisplayFor(model => model.SupplierEmail)
        </dd>

        <dt>
            @Miscellaneous.Description
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Discription)
        </dd>

        <dt>
            @Supplier.BranchTitle
        </dt>

        <dd>
            @Html.DisplayFor(model => model.BranchName)
        </dd>

        <dt>
            @Supplier.CompanyTitle
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CompanyName)
        </dd>

        <dt>
            @Localization.CloudERP.Modules.User.User.UserName
        </dt>

        <dd>
            @Html.DisplayFor(model => model.UserName)
        </dd>
    </dl>
</div>
