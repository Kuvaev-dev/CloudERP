@model Domain.Models.Supplier
@using Localization.CloudERP.Modules.Supplier;
@{
    ViewBag.Title = Supplier.SupplierDetails;
}

<div class="form-horizontal">
    <div class="col-md-10">
        <dl class="dl-horizontal">
            <dt>
                @Supplier.SupplierName
            </dt>

            <dd>
                @Html.DisplayFor(model => model.SupplierName)
            </dd>

            <dt>
                @Supplier.ConatctNo
            </dt>

            <dd>
                @Html.DisplayFor(model => model.SupplierConatctNo)
            </dd>

            <dt>
                @Supplier.Address
            </dt>

            <dd>
                @Html.DisplayFor(model => model.SupplierAddress)
            </dd>

            <dt>
                @Localization.CloudERP.Modules.User.User.Email
            </dt>

            <dd>
                @Html.DisplayFor(model => model.SupplierEmail)
            </dd>

            <dt>
                @Localization.CloudERP.Modules.Miscellaneous.Miscellaneous.Description
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Discription)
            </dd>

            <dt>
                @Supplier.BranchTitle
            </dt>

            <dd>
                @Html.DisplayFor(model => model.BranchName)
            </dd>

            <dt>
                @Supplier.CompanyTitle
            </dt>

            <dd>
                @Html.DisplayFor(model => model.CompanyName)
            </dd>

            <dt>
                @Localization.CloudERP.Modules.Sale.Sale.User
            </dt>

            <dd>
                @Html.DisplayFor(model => model.UserName)
            </dd>

        </dl>
    </div>
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            @Html.ActionLink(Localization.CloudERP.Common.Common.Edit, "Edit", new { id = Model.SupplierID }, new { @class = "btn btn-primary" })
            @Html.ActionLink(Localization.CloudERP.Common.Common.BackToList, "Index", null, new { @class = "btn btn-default" })
        </div>
    </div>
</div>