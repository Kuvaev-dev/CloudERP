@model Domain.Models.Supplier

@{
    ViewBag.Title = Localization.CloudERP.Supplier.SupplierDetails;
}

<div class="form-horizontal">
    <div class="col-md-10">
        <dl class="dl-horizontal">
            <dt>
                @Localization.CloudERP.Supplier.SupplierName
            </dt>

            <dd>
                @Html.DisplayFor(model => model.SupplierName)
            </dd>

            <dt>
                @Localization.CloudERP.Supplier.ConatctNo
            </dt>

            <dd>
                @Html.DisplayFor(model => model.SupplierConatctNo)
            </dd>

            <dt>
                @Localization.CloudERP.Supplier.Address
            </dt>

            <dd>
                @Html.DisplayFor(model => model.SupplierAddress)
            </dd>

            <dt>
                @Localization.CloudERP.Supplier.Email
            </dt>

            <dd>
                @Html.DisplayFor(model => model.SupplierEmail)
            </dd>

            <dt>
                @Localization.CloudERP.Supplier.Description
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Discription)
            </dd>

            <dt>
                @Localization.CloudERP.Supplier.BranchTitle
            </dt>

            <dd>
                @Html.DisplayFor(model => model.BranchName)
            </dd>

            <dt>
                @Localization.CloudERP.Supplier.CompanyTitle
            </dt>

            <dd>
                @Html.DisplayFor(model => model.CompanyName)
            </dd>

            <dt>
                @Localization.CloudERP.Supplier.User
            </dt>

            <dd>
                @Html.DisplayFor(model => model.UserName)
            </dd>

        </dl>
    </div>
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            @Html.ActionLink(Localization.CloudERP.Common.Edit, "Edit", new { id = Model.SupplierID }, new { @class="btn btn-primary" })
            @Html.ActionLink(Localization.CloudERP.Common.BackToList, "Index", null, new { @class="btn btn-default" })
        </div>
    </div>
</div>