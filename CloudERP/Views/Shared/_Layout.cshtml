<!DOCTYPE html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta name="description" content="Bootstrap Admin App + jQuery">
    <meta name="keywords" content="app, responsive, jquery, bootstrap, dashboard, admin">
    <title>@ViewBag.Title</title>
    <!-- =============== VENDOR STYLES ===============-->
    <!-- FONT AWESOME-->
    <link rel="stylesheet" href="~/Content/Template/vendor/font-awesome/css/font-awesome.css">
    <!-- SIMPLE LINE ICONS-->
    <link rel="stylesheet" href="~/Content/Template/vendor/simple-line-icons/css/simple-line-icons.css">
    <!-- ANIMATE.CSS-->
    <link rel="stylesheet" href="~/Content/Template/vendor/animate.css/animate.css">
    <!-- WHIRL (spinners)-->
    <link rel="stylesheet" href="~/Content/Template/vendor/whirl/dist/whirl.css">
    <!-- =============== PAGE VENDOR STYLES ===============-->
    <!-- Datatables-->
    <link rel="stylesheet" href="~/Content/Template/vendor/datatables.net-bs4/css/dataTables.bootstrap4.css">
    <link rel="stylesheet" href="~/Content/Template/vendor/datatables.net-keytable-bs/css/keyTable.bootstrap.css">
    <link rel="stylesheet" href="~/Content/Template/vendor/datatables.net-responsive-bs/css/responsive.bootstrap.css">
    <!-- =============== BOOTSTRAP STYLES ===============-->
    <link rel="stylesheet" href="~/Content/Template/css/bootstrap.css" id="bscss">
    <!-- =============== APP STYLES ===============-->
    <link rel="stylesheet" href="~/Content/Template/css/app.css" id="maincss">
    <!-- Bootgrid-->
    <link rel="stylesheet" href="~/Content/Template/vendor/jquery-bootgrid/dist/jquery.bootgrid.css">
    <!-- Icons-->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-icons/1.11.2/font/bootstrap-icons.min.css" />
    <!-- Themes-->
    <link rel="stylesheet" href="~/Scripts/Theme/dark-theme.css">
</head>

<body>
    <div class="wrapper">
        <!-- top navbar-->
        <header class="topnavbar-wrapper">
            <!-- START Top Navbar-->
            <nav class="navbar topnavbar">
                <!-- START navbar header-->
                <div class="navbar-header">
                    <a class="navbar-brand" href="#/">
                        <div class="brand-logo">
                            @{
                                string logo = string.Empty;
                                if (string.IsNullOrEmpty(Convert.ToString(Session["CLogo"])))
                                {
                                    logo = "~/Content/CompanyLogo/erp-logo.png";
                                }
                                else
                                {
                                    logo = Convert.ToString(Session["CLogo"]);
                                }
                            }
                            <img class="img-fluid" src="@Url.Content(logo)" alt="App Logo" width="30" height="30">
                        </div>
                        <div class="brand-logo-collapsed">
                            <img class="img-fluid" src="@Url.Content(logo)" alt="App Logo" width="30" height="30">
                        </div>
                    </a>
                </div>
                <!-- END navbar header-->
                <!-- START Left navbar-->
                <ul class="navbar-nav mr-auto flex-row">
                    <li class="nav-item">
                        <!-- Button used to collapse the left sidebar. Only visible on tablet and desktops-->
                        <a class="nav-link d-none d-md-block d-lg-block d-xl-block" href="#" data-trigger-resize="" data-toggle-state="aside-collapsed">
                            <em class="fa fa-navicon"></em>
                        </a>
                        <!-- Button to show/hide the sidebar on mobile. Visible on mobile only.-->
                        <a class="nav-link sidebar-toggle d-md-none" href="#" data-toggle-state="aside-toggled" data-no-persist="true">
                            <em class="fa fa-navicon"></em>
                        </a>
                    </li>
                    <!-- START User avatar toggle-->
                    <li class="nav-item">
                        <!-- Button used to collapse the left sidebar. Only visible on tablet and desktops-->
                        <a class="nav-link" id="user-block-toggle" href="#user-block" data-toggle="collapse">
                            <em class="icon-user"></em>
                        </a>
                    </li>
                    <!-- END User avatar toggle-->
                    <!-- START lock screen-->
                    <li class="nav-item">
                        <a class="nav-link" href="@Url.Action("Logout", "Home")" title="Lock screen">
                            <em class="icon-lock"></em>
                        </a>
                    </li>
                    <!-- END lock screen-->
                </ul>
                <!-- END Left navbar-->
                <!-- START Right Navbar-->
                <ul class="navbar-nav flex-row">
                    <!-- START Dark theme-->
                    <li class="nav-item">
                        <div class="nav-link container">
                            <i class="fas fa-moon"></i>
                        </div>
                    </li>
                    <!-- END Dark theme-->
                    <!-- START Language list-->
                    <li class="nav-item">
                        <div class="nav-link container">
                            <div class="dropdown">
                                <em class="fas fa-language dropdown-toggle" type="button" id="languageDropdown" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"></em>
                                <div class="dropdown-menu dropdown-menu-right" aria-labelledby="languageDropdown">
                                    <a class="dropdown-item" href="@Url.Action("SetCulture", "Home", new { culture = "en-US" })">English</a>
                                    <a class="dropdown-item" href="@Url.Action("SetCulture", "Home", new { culture = "ru-RU" })">Русский</a>
                                    <a class="dropdown-item" href="@Url.Action("SetCulture", "Home", new { culture = "uk-UA" })">Українська</a>
                                </div>
                            </div>
                        </div>
                    </li>
                    <!-- END Language list-->
                </ul>
                <!-- END Right Navbar-->
            </nav>
            <!-- END Top Navbar-->
        </header>
        <!-- sidebar-->
        <aside class="aside-container">
            <!-- START Sidebar (left)-->
            <div class="aside-inner">
                <nav class="sidebar" data-sidebar-anyclick-close="">
                    <!-- START sidebar nav-->
                    <ul class="sidebar-nav">
                        <!-- START user info-->
                        <li class="has-user-block">
                            <div class="collapse" id="user-block">
                                <div class="item user-block">
                                    <!-- User picture-->
                                    <div class="user-block-picture">
                                        <div class="user-block-status">
                                            @{
                                                string userImg = string.Empty;
                                                if (string.IsNullOrEmpty(Convert.ToString(Session["EPhoto"])))
                                                {
                                                    userImg = "~/Content/Template/img/user-default-img.png";
                                                }
                                                else
                                                {
                                                    userImg = Convert.ToString(Session["EPhoto"]);
                                                }
                                            }
                                            <img class="img-thumbnail rounded-circle" src="@Url.Content(userImg)" alt="Avatar" width="60" height="60">
                                            <div class="circle bg-success circle-lg"></div>
                                        </div>
                                    </div>
                                    <!-- Name and Job-->
                                    <div class="user-block-info">
                                        <span class="user-block-name">Hello, @Session["UserName"]</span>
                                        <span class="user-block-role">@Session["Designation"]</span>
                                    </div>
                                </div>
                            </div>
                        </li>
                        <!-- END user info-->
                        @{
                            if (Session["UserTypeID"] != null)
                            {
                                var userType = Convert.ToInt32(Convert.ToString(Session["UserTypeID"]));
                                if (Session["BranchTypeID"] != null)
                                {
                                    <!-- Iterates over all sidebar items-->

                                    @*Configuration*@
                                    @*Admin*@
                                    if (userType == 1)
                                    {
                                        <li class="nav-heading ">
                                            <span data-localize="sidebar.heading.COMPONENTS">Guide</span>
                                        </li>

                                        <li class="d-flex">
                                            <a href="@Url.Action("AdminMenuGuide", "Guide")" title="Admin Guide" class="d-flex align-items-center">
                                                <em class="bi bi-info-circle h4 mr-1"></em>
                                                <span class="d-flex align-items-center mb-2">Admin Guide</span>
                                            </a>
                                        </li>

                                        <li class="nav-heading ">
                                            <span data-localize="sidebar.heading.COMPONENTS">User Setting</span>
                                        </li>
                                        <li class="d-flex">
                                            <a href="@Url.Action("Index", "UserType")" title="User Types" class="d-flex align-items-center">
                                                <em class="bi bi-person-fill-gear h4 mr-1"></em>
                                                <span class="d-flex align-items-center mb-2">User Types</span>
                                            </a>
                                        </li>
                                        <li class="d-flex">
                                            <a href="@Url.Action("Index", "User")" title="Users" class="d-flex align-items-center">
                                                <em class="bi bi-person-fill h4 mr-1"></em>
                                                <span class="d-flex align-items-center mb-2">Users</span>
                                            </a>
                                        </li>
                                        <li class="nav-heading ">
                                            <span data-localize="sidebar.heading.COMPONENTS">Companie`s</span>
                                        </li>
                                        <li class="d-flex align-items-center">
                                            <a href="@Url.Action("Index", "Company")" title="Companies" class="d-flex align-items-center">
                                                <em class="bi bi-building h4 mr-1"></em>
                                                <span class="d-flex align-items-center mb-2">Companies</span>
                                            </a>
                                        </li>
                                        <li class="d-flex align-items-center">
                                            <a href="@Url.Action("RegistrationForm", "CompanyRegistration")" title="Company Registration" class="d-flex align-items-center">
                                                <em class="bi bi-building-add h4 mr-1"></em>
                                                <span class="d-flex align-items-center mb-2">Company Registration</span>
                                            </a> 
                                        </li>
                                        <li class="nav-heading ">
                                            <span data-localize="sidebar.heading.COMPONENTS">Account Settings</span>
                                        </li>
                                        <li class="d-flex align-items-center">
                                            <a href="@Url.Action("Index", "AccountHead")" title="Account Heads" class="d-flex align-items-center">
                                                <em class="bi bi-lightbulb-fill mr-1 mb-2"></em>
                                                <span class="d-flex align-items-center mb-2">Account Heads</span>
                                            </a>
                                        </li>
                                    }
                                    @*User*@
                                    if (userType == 2)
                                    {
                                        <li class="nav-heading ">
                                            <span data-localize="sidebar.heading.HEADER">Home</span>
                                        </li>
                                        <li class="d-flex align-items-center">
                                            <a href="@Url.Action("Index", "Home")" title="Dashboard" class="d-flex align-items-center">
                                                <em class="bi bi-speedometer2 h4 mr-1"></em>
                                                <span class="d-flex align-items-center mb-2">Dashboard</span>
                                            </a>
                                        </li>
                                        <li class="nav-heading ">
                                            <span data-localize="sidebar.heading.COMPONENTS">Sub Branch`s</span>
                                        </li>
                                        <li class="d-flex align-items-center">
                                            <a href="@Url.Action("Index", "Branch")" title="Branches" class="d-flex align-items-center">
                                                <em class="bi bi-diagram-2 h4 mr-1"></em>
                                                <span class="d-flex align-items-center mb-2">Branches</span>
                                            </a>
                                        </li>
                                        <li class="d-flex align-items-center">
                                            <a href="@Url.Action("SubBranchUser", "User")" title="Sub Branch`s Users" class="d-flex align-items-center">
                                                <em class="bi bi-person-fill h4 mr-1"></em>
                                                <span class="d-flex align-items-center mb-2">Users</span>
                                            </a>
                                        </li>

                                        var branchTypeID = Convert.ToInt32(Convert.ToString(Session["BranchTypeID"]));
                                        @*Main Branch*@
                                        if (branchTypeID == 1)
                                        {
                                            <li class="nav-heading ">
                                                <span data-localize="sidebar.heading.COMPONENTS">Guide</span>
                                            </li>
                                            <li class="d-flex align-items-center">
                                                <a href="@Url.Action("MainBranchEmployeeGuide", "Guide")" title="Main Employee Guide" class="d-flex align-items-center">
                                                    <em class="bi bi-info-circle h4 mr-1"></em>
                                                    <span class="d-flex align-items-center mb-2">Main Employee Guide</span>
                                                </a>
                                            </li>
                                            <li class="nav-heading ">
                                                <span data-localize="sidebar.heading.COMPONENTS">Branch`s</span>
                                            </li>
                                            <li class="d-flex align-items-center">
                                                <a href="@Url.Action("Create", "Branch")" title="New Branch" class="d-flex align-items-center">
                                                    <em class="bi bi-diagram-2 h4 mr-1"></em>
                                                    <span class="d-flex align-items-center mb-2">New Branch</span>
                                                </a>
                                            </li>
                                            <li class="nav-heading ">
                                                <span data-localize="sidebar.heading.COMPONENTS">Employee`s</span>
                                            </li>
                                            <li class="d-flex align-items-center">
                                                <a href="@Url.Action("Employees", "CompanyEmployee")" title="Employee Registration" class="d-flex align-items-center">
                                                    <em class="bi bi-person-add h4 mr-1"></em>
                                                    <span class="d-flex align-items-center mb-2">Employee Registration</span>
                                                </a>
                                            </li>
                                            <li class="d-flex align-items-center">
                                                <a href="@Url.Action("EmployeeSalary", "CompanyEmployee")" title="Payroll" class="d-flex align-items-center">
                                                    <em class="bi bi-coin h4 mr-1"></em>
                                                    <span class="d-flex align-items-center mb-2">Payroll</span>
                                                </a>
                                            </li>
                                            <li class="d-flex align-items-center">
                                                <a href="@Url.Action("SalaryHistory", "CompanyEmployee")" title="Paid Salaries History" class="d-flex align-items-center">
                                                    <em class="bi bi-cash-stack h4 mr-1"></em>
                                                    <span class="d-flex align-items-center mb-2">Paid Salaries History</span>
                                                </a>
                                            </li>
                                        }
                                        if (branchTypeID > 1)
                                        {
                                            <li class="nav-heading ">
                                                <span data-localize="sidebar.heading.COMPONENTS">Guide</span>
                                            </li>
                                            <li class="d-flex align-items-center">
                                                <a href="@Url.Action("EmployeeGuide", "Guide")" title="Employee Guide" class="d-flex align-items-center">
                                                    <em class="bi bi-info-circle h4 mr-1"></em>
                                                    <span class="d-flex align-items-center mb-2">Employee Guide</span>
                                                </a>
                                            </li>
                                            <li class="nav-heading ">
                                                <span data-localize="sidebar.heading.COMPONENTS">Employee`s</span>
                                            </li>
                                            <li class="d-flex align-items-center">
                                                <a href="@Url.Action("Employee", "BranchEmployee")" title="Employee Registration" class="d-flex align-items-center">
                                                    <em class="bi bi-person-add h4 mr-1"></em>
                                                    <span class="d-flex align-items-center mb-2">Employee Registration</span>
                                                </a>
                                            </li>
                                            <li class="d-flex align-items-center">
                                                <a href="@Url.Action("EmployeeSalary", "CompanyEmployee")" title="Payroll" class="d-flex align-items-center">
                                                    <em class="bi bi-coin h4 mr-1"></em>
                                                    <span class="d-flex align-items-center mb-2">Payroll</span>
                                                </a>
                                            </li>
                                            <li class="d-flex align-items-center">
                                                <a href="@Url.Action("SalaryHistory", "CompanyEmployee")" title="Paid Salaries History" class="d-flex align-items-center">
                                                    <em class="bi bi-cash-stack h4 mr-1"></em>
                                                    <span class="d-flex align-items-center mb-2">Paid Salaries History</span>
                                                </a>
                                            </li>
                                        }
                                        <li class="nav-heading ">
                                            <span data-localize="sidebar.heading.COMPONENTS">Stock`s</span>
                                        </li>
                                        <li class="d-flex align-items-center">
                                            <a href="@Url.Action("Index", "Category")" title="Stock Categories" class="d-flex align-items-center">
                                                <em class="bi bi-inboxes-fill h4 mr-1"></em>
                                                <span class="d-flex align-items-center mb-2">Stock Categories</span>
                                            </a>
                                        </li>
                                        <li class="d-flex align-items-center">
                                            <a href="@Url.Action("Index", "Stock")" title="Stock Details" class="d-flex align-items-center">
                                                <em class="bi bi-boxes h4 mr-1"></em>
                                                <span class="d-flex align-items-center mb-2">Stock Details</span>
                                            </a>
                                        </li>
                                    }
                                    <li class="nav-heading ">
                                        <span data-localize="sidebar.heading.COMPONENTS">Account Settings</span>
                                    </li>
                                    <li class="d-flex align-items-center">
                                        <a href="@Url.Action("Index", "AccountControl")" title="Account Controls" class="d-flex align-items-center">
                                            <em class="bi bi-gear-wide h4 mr-1"></em>
                                            <span class="d-flex align-items-center mb-2">Account Controls</span>
                                        </a>
                                    </li>
                                    <li class="d-flex align-items-center">
                                        <a href="@Url.Action("Index", "AccountSubControl")" title="Account Sub Controls" class="d-flex align-items-center">
                                            <em class="bi bi-gear-wide-connected h4 mr-1"></em>
                                            <span class="d-flex align-items-center mb-2">Account Sub Controls</span>
                                        </a>
                                    </li>
                                    <li class="d-flex align-items-center">
                                        <a href="@Url.Action("Index", "AccountSetting")" title="Account Flow" class="d-flex align-items-center">
                                            <em class="bi bi-person-lines-fill h4 mr-1"></em>
                                            <span class="d-flex align-items-center mb-2">Account Flow</span>
                                        </a>
                                    </li>
                                }

                                if (userType == 1)
                                {
                                    <li class="nav-heading ">
                                        <span>Financial Year Settings</span>
                                    </li>
                                    <li class="d-flex align-items-center">
                                        <a href="@Url.Action("Index", "FinancialYear")" title="Financial Years" class="d-flex align-items-center">
                                            <em class="bi bi-cash h4 mr-1"></em>
                                            <span class="d-flex align-items-center mb-2">Financial Years</span>
                                        </a>
                                    </li>
                                    <li class="d-flex align-items-center">
                                        <a href="@Url.Action("Create", "FinancialYear")" title="New Financial Year" class="d-flex align-items-center">
                                            <em class="bi bi-cash-coin h4 mr-1"></em>
                                            <span class="d-flex align-items-center mb-2">New Financial Year</span>
                                        </a>
                                    </li>
                                }
                                if (userType == 2)
                                {
                                    <li class="nav-heading ">
                                        <span data-localize="sidebar.heading.COMPONENTS">Financial Year`s</span>
                                    </li>
                                    <li class="d-flex align-items-center">
                                        <a href="@Url.Action("Index", "FinancialYearView")" title="All Financial Years" class="d-flex align-items-center">
                                            <em class="bi bi-calendar3 h4 mr-1"></em>
                                            <span class="d-flex align-items-center mb-2">All Financial Years</span>
                                        </a>
                                    </li>
                                }
                                <li class="nav-heading ">
                                    <span data-localize="sidebar.heading.COMPONENTS">Customers</span>
                                </li>
                                if (userType == 2)
                                {
                                    <li class="d-flex align-items-center">
                                        <a href="@Url.Action("Create", "Customer")" title="New Customer" class="d-flex align-items-center">
                                            <em class="bi bi-person-fill-add h4 mr-1"></em>
                                            <span class="d-flex align-items-center mb-2">New Customer</span>
                                        </a>
                                    </li>
                                    <li class="d-flex align-items-center">
                                        <a href="@Url.Action("Index", "Customer")" title="Branch Customers" class="d-flex align-items-center">
                                            <em class="bi bi-cart h4 mr-1"></em>
                                            <span class="d-flex align-items-center mb-2">Branch Customers</span>
                                        </a>
                                    </li>
                                    <li class="d-flex align-items-center">
                                        <a href="@Url.Action("SubBranchCustomer", "Customer")" title="Sub Branch Customers" class="d-flex align-items-center">
                                            <em class="bi bi-cart-check h4 mr-1"></em>
                                            <span class="d-flex align-items-center mb-2">Sub Branch Customers</span>
                                        </a>
                                    </li>
                                    <li class="nav-heading ">
                                        <span data-localize="sidebar.heading.COMPONENTS">Sale`s</span>
                                    </li>
                                    <li class="d-flex align-items-center">
                                        <a href="@Url.Action("NewSale", "SaleCart")" title="New Sale" class="d-flex align-items-center">
                                            <em class="bi bi-bag-plus h4 mr-1"></em>
                                            <span class="d-flex align-items-center mb-2">New Sale</span>
                                        </a>
                                    </li>
                                    <li class="d-flex align-items-center">
                                        <a href="@Url.Action("RemainingPaymentList", "SalePayment")" title="Sale Pending Payment" class="d-flex align-items-center">
                                            <em class="bi bi-paypal h4 mr-1"></em>
                                            <span class="d-flex align-items-center mb-2">Sale Pending Payment</span>
                                        </a>
                                    </li>
                                    <li class="d-flex align-items-center">
                                        <a href="@Url.Action("CustomSalesHistory", "SalePayment", new { FromDate = DateTime.Now, ToDate = DateTime.Now })" title="All Sales" class="d-flex align-items-center">
                                            <em class="bi bi-card-checklist h4 mr-1"></em>
                                            <span class="d-flex align-items-center mb-2">All Sales</span>
                                        </a>
                                    </li>
                                    <li class="d-flex align-items-center">
                                        <a href="@Url.Action("FindSale", "SaleReturn")" title="Sale Return" class="d-flex align-items-center">
                                            <em class="bi bi-bag-dash h4 mr-1"></em>
                                            <span class="d-flex align-items-center mb-2">Sale Return</span>
                                        </a>
                                    </li>
                                    <li class="d-flex align-items-center">
                                        <a href="@Url.Action("AllReturnSalesPendingAmount", "SalePaymentReturn")" title="Return Sales Pending Amount" class="d-flex align-items-center">
                                            <em class="bi bi-wallet-fill h4 mr-1"></em>
                                            <span class="d-flex align-items-center mb-2">Return Sales Pending Amount</span>
                                        </a>
                                    </li>
                                }
                                if (userType == 1)
                                {
                                    <li class="d-flex align-items-center">
                                        <a href="@Url.Action("AllCustomers", "Customer")" title="All Customers" class="d-flex align-items-center">
                                            <em class="bi bi-cart h4 mr-1"></em>
                                            <span class="d-flex align-items-center mb-2">All Customers</span>
                                        </a>
                                    </li>
                                }
                                <li class="nav-heading ">
                                    <span data-localize="sidebar.heading.COMPONENTS">Suppliers</span>
                                </li>
                                if (userType == 2)
                                {
                                    <li class="d-flex align-items-center">
                                        <a href="@Url.Action("Create", "Supplier")" title="New Supplier" class="d-flex align-items-center">
                                            <em class="bi bi-umbrella h4 mr-1"></em>
                                            <span class="d-flex align-items-center mb-2">New Supplier</span>
                                        </a>
                                    </li>
                                    <li class="d-flex align-items-center">
                                        <a href="@Url.Action("Index", "Supplier")" title="Branch Suppliers" class="d-flex align-items-center">
                                            <em class="bi bi-truck h4 mr-1"></em>
                                            <span class="d-flex align-items-center mb-2">Branch Suppliers</span>
                                        </a>
                                    </li>
                                    <li class="d-flex align-items-center">
                                        <a href="@Url.Action("SubBranchSupplier", "Supplier")" title="Sub Branch Suppliers" class="d-flex align-items-center">
                                            <em class="bi bi-truck-front h4 mr-1"></em>
                                            <span class="d-flex align-items-center mb-2">Sub Branch Suppliers</span>
                                        </a>
                                    </li>
                                    <li class="nav-heading ">
                                        <span data-localize="sidebar.heading.COMPONENTS">Purchase`s</span>
                                    </li>
                                    <li class="d-flex align-items-center">
                                        <a href="@Url.Action("NewPurchase", "PurchaseCart")" title="New Purchase" class="d-flex align-items-center">
                                            <em class="bi bi-cart-plus h4 mr-1"></em>
                                            <span class="d-flex align-items-center mb-2">New Purchase</span>
                                        </a>
                                    </li>
                                    <li class="d-flex align-items-center">
                                        <a href="@Url.Action("RemainingPaymentList", "PurchasePayment")" title="Purchase Pending Payment" class="d-flex align-items-center">
                                            <em class="bi bi-currency-dollar h4 mr-1"></em>
                                            <span class="d-flex align-items-center mb-2">Purchase Pending Payment</span>
                                        </a>
