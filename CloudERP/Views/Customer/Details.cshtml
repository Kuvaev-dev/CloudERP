@model Domain.Models.Customer
@using Localization.CloudERP.Modules.Miscellaneous
@using Localization.CloudERP.Modules.Company
@using Localization.CloudERP.Modules.Branch
@{
    ViewBag.Title = Miscellaneous.BranchCustomers;
}

<div>
    <dl class="dl-horizontal">
        <dt>
            @Miscellaneous.CustomerFullName
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Customername)
        </dd>

        <dt>
            @Localization.CloudERP.Modules.User.User.ContactNo
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CustomerContact)
        </dd>

        <dt>
            @Localization.CloudERP.Modules.Supplier.Supplier.Address
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CustomerAddress)
        </dd>

        <dt>
            @Miscellaneous.Description
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Description)
        </dd>

        <dt>
            @Branch.BranchName
        </dt>

        <dd>
            @Html.DisplayFor(model => model.BranchName)
        </dd>

        <dt>
            @Company.CompanyTitle
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CompanyName)
        </dd>

        <dt>
            @Localization.CloudERP.Modules.User.User.UserName
        </dt>

        <dd>
            @Html.DisplayFor(model => model.UserName)
        </dd>
    </dl>
</div>
